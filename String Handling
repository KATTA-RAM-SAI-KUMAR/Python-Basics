s=input("Enter the string: ")
upper,lower,symbol,digit,space=0,0,0,0,0
for i in s:
    if i==" ":
        space=space+1
    elif i.isdigit():
        digit=digit+1
    elif i.isupper():
        upper=upper+1
    elif i.islower():
        lower=lower+1
    else:
        symbol=symbol+1
        
print("Spaces: ",space)
print("lower case : ",lower)
print("Upper case : ",upper)
print("digits : ",digit)
print("Symbols : ",symbol)
